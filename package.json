{
  "name": "excalibur",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "migrate:dev": "truffle migrate --network development --reset",
    "migrate:testnet": "truffle migrate --network testnet --reset",
    "migrate:bsc": "truffle migrate --network bsc --dry-run",
    "compile": "truffle compile --network bscTestnet",
    "encode": "ts-node ./scripts/encode.ts",
    "test": "npx truffle test",
    "gas-report": "env ENABLE_GAS_REPORT=true npm run test",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:fix": "npm run lint:js:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solhint 'contracts/**/*.sol' && prettier -c 'contracts/**/*.sol'",
    "lint:sol:fix": "prettier --write \"contracts/**/*.sol\""
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.0.14",
    "@ethersproject/contracts": "^5.0.11",
    "@openzeppelin/contracts": "^3.4.1-solc-0.7",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@truffle/hdwallet-provider": "^1.2.6",
    "@uniswap/lib": "1.1.1",
    "async": "^3.2.0",
    "dotenv": "^8.2.0",
    "excalibur": "../farm",
    "truffle": "^5.3.2",
    "truffle-flattener": "^1.4.4",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.7",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-waffle": "^2.0.0",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@openzeppelin/truffle-upgrades": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eth-gas-reporter": "^0.2.22",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.14",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.13",
    "truffle-plugin-verify": "^0.5.7",
    "ts-node": "^9.1.1",
    "web3": "^1.3.0"
  }
}
